<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <TextView
            android:layout_width="345dp"
            android:layout_height="71dp"
            android:text="회원 관리"
            android:textSize="30dp"
            android:gravity="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.421"
            app:layout_constraintVertical_bias="0.043" android:id="@+id/textView"/>
    <Button
            android:text="회원추가"
            android:layout_width="143dp"
            android:layout_height="64dp" android:id="@+id/insertBtn"
            app:layout_constraintTop_toBottomOf="@+id/textView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="68dp" android:layout_marginStart="204dp"/>
    <Button
            android:text="회원삭제"
            android:layout_width="143dp"
            android:layout_height="64dp" android:id="@+id/deleteBtn"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="204dp" app:layout_constraintTop_toBottomOf="@+id/insertBtn"
            android:layout_marginTop="68dp"/>
    <Button
            android:text="Button"
            android:layout_width="143dp"
            android:layout_height="64dp" android:id="@+id/button3"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="204dp" app:layout_constraintTop_toBottomOf="@+id/deleteBtn"
            android:layout_marginTop="72dp"/>
    <Button
            android:text="Button"
            android:layout_width="143dp"
            android:layout_height="64dp" android:id="@+id/button4"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="204dp" app:layout_constraintTop_toBottomOf="@+id/deleteBtn"
            android:layout_marginTop="188dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>