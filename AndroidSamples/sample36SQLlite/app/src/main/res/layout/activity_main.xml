<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <EditText
            android:layout_width="494dp"
            android:layout_height="48dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editInsert"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.377" app:layout_constraintVertical_bias="0.043"/>
    <EditText
            android:layout_width="501dp"
            android:layout_height="55dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editDelete"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toBottomOf="@+id/editInsert" android:layout_marginTop="152dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.311"/>
    <EditText
            android:layout_width="369dp"
            android:layout_height="67dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editSelect"
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toBottomOf="@+id/editInsert" android:layout_marginTop="372dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.372"/>
    <EditText
            android:layout_width="158dp"
            android:layout_height="49dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editUpdate"
            android:text=""
            tools:ignore="MissingConstraints"
            app:layout_constraintTop_toBottomOf="@+id/editInsert" android:layout_marginTop="572dp"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.696"/>
    <Button
            android:text="추가"
            android:layout_width="246dp"
            android:layout_height="79dp" android:id="@+id/insertBtn"
            app:layout_constraintTop_toBottomOf="@+id/editInsert" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="28dp" android:layout_marginStart="156dp"/>
    <Button
            android:text="삭제"
            android:layout_width="246dp"
            android:layout_height="79dp" android:id="@+id/deleteBtn"
            app:layout_constraintTop_toBottomOf="@+id/editDelete" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="144dp" android:layout_marginTop="52dp"/>
    <Button
            android:text="검색"
            android:layout_width="246dp"
            android:layout_height="79dp" android:id="@+id/selectBtn"
            app:layout_constraintTop_toBottomOf="@+id/editSelect" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="156dp" android:layout_marginTop="28dp"/>
    <Button
            android:text="수정하기"
            android:layout_width="246dp"
            android:layout_height="79dp" android:id="@+id/updateBtn"
            app:layout_constraintTop_toBottomOf="@+id/editUpdate" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="144dp" android:layout_marginTop="20dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="133dp"
            android:layout_height="38dp" android:id="@+id/selectText"
            app:layout_constraintTop_toBottomOf="@+id/editInsert"
            android:layout_marginTop="580dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="64dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>