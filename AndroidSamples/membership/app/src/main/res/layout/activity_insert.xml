<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".InsertActivity">

    <TextView
            android:id="@+id/insertTextView"
            android:layout_width="244dp"
            android:layout_height="54dp"
            android:text="회원추가"
            android:textAlignment="center"
            android:textSize="35sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.06"/>
    <TextView
            android:text="이름"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="30sp"
            android:textAlignment="center"
            android:id="@+id/nameText"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.065"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="64dp"
            app:layout_constraintTop_toBottomOf="@+id/insertTextView"/>
    <TextView
            android:text="나이"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="30sp"
            android:textAlignment="center"
            android:id="@+id/ageText"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.202"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="64dp"
            app:layout_constraintTop_toBottomOf="@+id/insertTextView"/>
    <Button
            android:text="회원 추가"
            android:layout_width="187dp"
            android:layout_height="79dp" android:id="@+id/addBtn"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/insertTextView" android:layout_marginTop="708dp"
            app:layout_constraintHorizontal_bias="0.135"/>
    <Button
            android:text="돌아가기"
            android:layout_width="187dp"
            android:layout_height="79dp" android:id="@+id/returnBtn"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/insertTextView" android:layout_marginTop="708dp"
            app:layout_constraintHorizontal_bias="0.779"/>
    <EditText
            android:layout_width="325dp"
            android:layout_height="55dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editTextName"
            app:layout_constraintStart_toEndOf="@+id/nameText" android:layout_marginStart="48dp"
            app:layout_constraintTop_toTopOf="@+id/nameText"/>
    <EditText
            android:layout_width="325dp"
            android:layout_height="55dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/editTextAge"
            app:layout_constraintStart_toEndOf="@+id/ageText"
            app:layout_constraintTop_toTopOf="@+id/ageText" android:layout_marginStart="36dp"/>
    <EditText
            android:layout_width="325dp"
            android:layout_height="55dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editTextAddress"
            app:layout_constraintStart_toEndOf="@+id/addressText"
            app:layout_constraintTop_toTopOf="@+id/addressText" android:layout_marginStart="48dp"
    />
    <EditText
            android:layout_width="325dp"
            android:layout_height="55dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/editTextPhone"
            app:layout_constraintStart_toEndOf="@+id/addressText"
            app:layout_constraintTop_toTopOf="@+id/addressText" android:layout_marginStart="84dp"
            android:layout_marginTop="92dp"/>
    <EditText
            android:layout_width="325dp"
            android:layout_height="55dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editTextJob"
            app:layout_constraintStart_toEndOf="@+id/addressText"
            app:layout_constraintTop_toTopOf="@+id/addressText3" android:layout_marginStart="36dp"
    />
    <EditText
            android:layout_width="325dp"
            android:layout_height="55dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editTextRelationship"
            app:layout_constraintStart_toEndOf="@+id/addressText"
            app:layout_constraintTop_toTopOf="@+id/addressText2" android:layout_marginStart="36dp"
    />
    <TextView
            android:text="주소"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="30sp"
            android:textAlignment="center"
            android:id="@+id/addressText"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.588"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="56dp"
            app:layout_constraintTop_toBottomOf="@+id/insertTextView"/>
    <TextView
            android:text="전화번호"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="30sp"
            android:textAlignment="center"
            android:id="@+id/addressText4"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.693"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="56dp"
            app:layout_constraintTop_toBottomOf="@+id/insertTextView"/>
    <TextView
            android:text="직업"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="30sp"
            android:textAlignment="center"
            android:id="@+id/addressText3"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.448"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="56dp"
            app:layout_constraintTop_toBottomOf="@+id/insertTextView"/>
    <TextView
            android:text="관계"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="30sp"
            android:textAlignment="center"
            android:id="@+id/addressText2"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.312"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="64dp"
            app:layout_constraintTop_toBottomOf="@+id/insertTextView"/>

</androidx.constraintlayout.widget.ConstraintLayout>