<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".allPrintActivity">

    <TextView
            android:id="@+id/apText"
            android:layout_width="244dp"
            android:layout_height="54dp"
            android:text="모든 회원 정보"
            android:textAlignment="center"
            android:textSize="35sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.06"/>
    <TextView
            android:text="TextView"
            android:layout_width="405dp"
            android:layout_height="652dp" android:id="@+id/apResText"
            android:textSize="20sp"
            app:layout_constraintTop_toBottomOf="@+id/apText" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="44dp"
            app:layout_constraintHorizontal_bias="0.502"/>
</androidx.constraintlayout.widget.ConstraintLayout>