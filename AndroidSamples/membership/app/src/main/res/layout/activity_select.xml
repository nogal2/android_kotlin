<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".SelectActivity">

    <EditText
            android:layout_width="325dp"
            android:layout_height="55dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/selEditTextName"
            tools:ignore="SpeakableTextPresentCheck"
            app:layout_constraintTop_toTopOf="@+id/textView4" app:layout_constraintStart_toEndOf="@+id/textView4"
            android:layout_marginStart="28dp"/>
    <TextView
            android:id="@+id/selectTextView"
            android:layout_width="244dp"
            android:layout_height="54dp"
            android:text="회원검색"
            android:textAlignment="center"
            android:textSize="35sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.06"/>
    <TextView
            android:text="검색결과"
            android:layout_width="407dp"
            android:layout_height="325dp" android:id="@+id/selResTextView"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toBottomOf="@+id/selectTextView"
            app:layout_constraintHorizontal_bias="0.502" app:layout_constraintVertical_bias="0.461"/>
    <Button
            android:text="회원검색"
            android:layout_width="214dp"
            android:layout_height="93dp" android:id="@+id/btnSelect"
            app:layout_constraintTop_toBottomOf="@+id/selResTextView"
            android:layout_marginTop="32dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="돌아가기"
            android:layout_width="214dp"
            android:layout_height="93dp" android:id="@+id/selReturnBtn"
            app:layout_constraintTop_toBottomOf="@+id/selResTextView"
            android:layout_marginTop="144dp" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:text="이름"
            android:textSize="30sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView4"
            app:layout_constraintTop_toBottomOf="@+id/selectTextView" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="80dp" android:layout_marginStart="84dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>